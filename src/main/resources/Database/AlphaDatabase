CREATE TABLE Pre_Order_Manage (
    pre_order_id VARCHAR(10) NOT NULL,
    user_id VARCHAR(10) NOT NULL,
    item_id VARCHAR(500) NOT NULL,
    advance_payment DOUBLE(10,2) NOT NULL,
    CONSTRAINT PRIMARY KEY (pre_order_id),
    CONSTRAINT FOREIGN KEY (user_id) REFERENCES User(user_id)
    ON UPDATE CASCADE ON DELETE CASCADE,
    CONSTRAINT FOREIGN KEY (item_id) REFERENCES Item(item_id)
    ON UPDATE CASCADE ON DELETE CASCADE
);

