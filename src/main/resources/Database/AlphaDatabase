('U003', 'PasinduUpamal', 'pasindupamal@gmail.com','Upamal123@' ,  '0714703970','Ragala', 'cashier');

CREATE TABLE Employee (
    emp_id VARCHAR(10) NOT NULL,
    emp_name VARCHAR(150) NOT NULL,
    emp_contact VARCHAR(15),
    emp_address VARCHAR(200),
    emp_nic VARCHAR(12) NOT NULL,
    emp_age INT,
    salary DECIMAL(10,2),
    CONSTRAINT PRIMARY KEY (emp_nic)
);

