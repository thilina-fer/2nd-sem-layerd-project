CREATE TABLE Supplier (
    supplier_id VARCHAR(10) NOT NULL,
    supplier_name VARCHAR(150) NOT NULL,
    supplier_contact VARCHAR(10),
    supplier_address VARCHAR(100),
    CONSTRAINT PRIMARY KEY (supplier_id)
);

