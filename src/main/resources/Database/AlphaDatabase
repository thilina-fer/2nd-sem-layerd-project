    customer_id VARCHAR(10) NOT NULL,
    customer_name VARCHAR(150) NOT NULL,
    customer_contact VARCHAR(10),
    customer_address VARCHAR(200),
    CONSTRAINT PRIMARY KEY (customer_contact)
);

