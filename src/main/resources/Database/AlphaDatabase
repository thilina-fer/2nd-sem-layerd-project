CREATE TABLE Item (
    item_id VARCHAR(500) NOT NULL,
    item_name VARCHAR(300) NOT NULL,
    quantity INT NOT NULL,
    buying_price DECIMAL(10,2) NOT NULL,
    selling_price DECIMAL(10,2) NOT NULL,
    CONSTRAINT PRIMARY KEY (item_id)
);

